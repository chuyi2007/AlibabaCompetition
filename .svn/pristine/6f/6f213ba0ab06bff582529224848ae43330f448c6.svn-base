package home.s1.data;

public class MyDate {
	private int month;
	private int day;
	public MyDate(int month, int day) {
		super();
		this.setMonth(month);
		this.setDay(day);
	}
	public int getMonth() {
		return month;
	}
	public void setMonth(int month) {
		this.month = month;
	}
	public int getDay() {
		return day;
	}
	public void setDay(int day) {
		this.day = day;
	}
	
}
